    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.2.4",
    "storybook": "^7.6.10",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
